    tiles.setWallAt(tiles.getTileLocation(14, 13), false)
    tiles.setWallAt(tiles.getTileLocation(14, 4), false)
    tiles.setWallAt(tiles.getTileLocation(8, 5), false)
    tiles.setWallAt(tiles.getTileLocation(8, 9), false)
    tiles.setWallAt(tiles.getTileLocation(10, 7), false)
