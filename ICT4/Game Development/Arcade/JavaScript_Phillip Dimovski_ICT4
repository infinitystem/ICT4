

enum ActionKind {
    Walking,
    Idle,
    Jumping,
    walkingup,
    idlebackofhead,
    leftrun,
    rightrun,
    runidle,
    leftidle,
    Civ,
    help
}
namespace SpriteKind {
    export const Save = SpriteKind.create()
    export const Person = SpriteKind.create()
    export const Saved = SpriteKind.create()
    export const keyY = SpriteKind.create()
    export const keyP = SpriteKind.create()
    export const keyR = SpriteKind.create()
    export const keyB = SpriteKind.create()
    export const keyG = SpriteKind.create()
    export const Rock = SpriteKind.create()
    export const ENemy1 = SpriteKind.create()
    export const bossfight = SpriteKind.create()
}
