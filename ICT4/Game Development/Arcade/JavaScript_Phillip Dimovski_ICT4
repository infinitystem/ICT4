})
sprites.onOverlap(SpriteKind.Player, SpriteKind.keyP, function (sprite, otherSprite) {
    music.jumpUp.play()
    scene.cameraShake(3, 500)
    tiles.setWallAt(tiles.getTileLocation(5, 7), false)
    tiles.setWallAt(tiles.getTileLocation(14, 4), false)
    tiles.setWallAt(tiles.getTileLocation(14, 7), false)
    tiles.setWallAt(tiles.getTileLocation(8, 5), false)
    tiles.setWallAt(tiles.getTileLocation(8, 9), false)
    tiles.setWallAt(tiles.getTileLocation(10, 7), false)
    tiles.setWallAt(tiles.getTileLocation(4, 7), false)
    Pkey.destroy(effects.spray, 500)
})
